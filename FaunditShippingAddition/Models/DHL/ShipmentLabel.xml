<?xml version="1.0" encoding="UTF-8"?>
<req:ShipmentRequest xmlns:req="http://www.dhl.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dhl.com ship-val-global-req.xsd" schemaVersion="6.0">
  <Request>
    <ServiceHeader>
      <MessageTime>2002-08-20T11:28:56.000-08:00</MessageTime>
      <MessageReference>1234567890123456789012345678901</MessageReference>
      <SiteID>%%SITEID%%</SiteID>
      <Password>%%PASSWORD%%</Password>
    </ServiceHeader>
  </Request>
  <NewShipper>N</NewShipper>
  <LanguageCode>en</LanguageCode>
  <PiecesEnabled>Y</PiecesEnabled>
  <Billing>
    <ShipperAccountNumber>%%SHIPPERACCOUNTNUMBER%%</ShipperAccountNumber>
    <ShippingPaymentType>S</ShippingPaymentType>
  </Billing>
  <Consignee>
    <CompanyName>%%CONSIGNEE_COMPANYNAME%%</CompanyName>
    <AddressLine>%%CONSIGNEE_ADDRESS1%%</AddressLine>
    <AddressLine>%%CONSIGNEE_ADDRESS2%%</AddressLine>
    <City>%%CONSIGNEE_CITY%%</City>
    <PostalCode>%%CONSIGNEE_POSTALCODE%%</PostalCode>
    <CountryCode>%%CONSIGNEE_COUNTRYCODE%%</CountryCode>
    <CountryName>%%CONSIGNEE_COUNTRYNAME%%</CountryName>
    <Contact>
      <PersonName>%%CONSIGNEE_CONTACT_PERSONNAME%%</PersonName>
      <PhoneNumber>%%CONSIGNEE_CONTACT_PHONENUMBER%%</PhoneNumber>
    </Contact>
  </Consignee>
  <Dutiable>
    <DeclaredValue>%%CUSTOM_VALUE%%</DeclaredValue>
    <DeclaredCurrency>DKK</DeclaredCurrency>
    <TermsOfTrade>DAP</TermsOfTrade>
  </Dutiable>
  <Reference>
    <ReferenceID>%%ITEMNUMBER%%</ReferenceID>
    <ReferenceType>CU</ReferenceType>
  </Reference>
  <ShipmentDetails>
    <NumberOfPieces>1</NumberOfPieces>
    <Pieces>
      <Piece>
        <PieceID>1</PieceID>
        <PackageType>EE</PackageType>
        <Weight>%%WEIGHT%%</Weight>
        <Width>20</Width>
        <Height>20</Height>
        <Depth>20</Depth>
      </Piece>
    </Pieces>
    <Weight>%%WEIGHT%%</Weight>
    <WeightUnit>K</WeightUnit>
    <GlobalProductCode>%%SHIPMENT_TYPE%%</GlobalProductCode>
    <!--<ProductContentCode>%%SHIPMENT_TYPE2%%</ProductContentCode>-->
    <Date>%%PAYMENT_DATE%%</Date>
    <Contents>%%ITEM_NAME%%</Contents>
    <DimensionUnit>C</DimensionUnit>
    <InsuredAmount>%%INSURENCE_VALUE%%</InsuredAmount>
    <PackageType>EE</PackageType>
    <CurrencyCode>DKK</CurrencyCode>

    <!--<IsDutiable>%%DUTY%%</IsDutiable>-->
  </ShipmentDetails>
  <Shipper>
    <ShipperID>%%SHIPPERID%%</ShipperID>
    <CompanyName>%%SHIPPERNAME%%</CompanyName>
    <RegisteredAccount>%%REGISTEREDACCOUNT%%</RegisteredAccount>
    <AddressLine>%%SHIPPER_ADDRESS1%%</AddressLine>
    <AddressLine>%%SHIPPER_ADDRESS2%%</AddressLine>
    <City>%%SHIPPER_CITY%%</City>
    <PostalCode>%%SHIPPER_POSTALCODE%%</PostalCode>
    <CountryCode>%%SHIPPER_COUNTRYCODE%%</CountryCode>
    <CountryName>%%SHIPPER_COUNTRYNAME%%</CountryName>
    <Contact>
      <PersonName>%%SHIPPERNAME%%</PersonName>
      <PhoneNumber>%%SHIPPER_PHONENUMBER%%</PhoneNumber>
    </Contact>
  </Shipper>

  <SpecialService>
    <SpecialServiceType>%%SPECIAL_SERVICE_TYPE%%</SpecialServiceType>
  </SpecialService>

  <EProcShip>N</EProcShip>
  <LabelImageFormat>PDF</LabelImageFormat>
  <RequestArchiveDoc>Y</RequestArchiveDoc>
</req:ShipmentRequest>